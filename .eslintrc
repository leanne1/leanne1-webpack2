{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
        "browser": true
    },
    "rules": {
        "indent": ["error", "tab", { "SwitchCase": 1 }],
        "no-tabs": 0,
        "arrow-parens": 0,
        "react/jsx-filename-extension": 0,
        "react/jsx-indent": ["error", "tab"]
    }
}
